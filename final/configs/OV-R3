conf t

 hostname "OV-R3"

 ipv6 unicast-routing

 interface Loopback64501
  description "loopback"
  ip address 10.0.3.3 255.255.255.255
  ip ospf authentication message-digest
  ip ospf message-digest-key 100 md5 ospfpassarea0
  ipv6 enable
  ipv6 address FD00:CCFF:200D::3/128
  ipv6 address FE80::3 link-local
  ipv6 ospf authentication ipsec spi 500 md5 1234567890abcdef1234567890abcdef
  ipv6 ospf 64501 area 0
  no shutdown
  exit

 interface Ethernet0/0
  description "to OV-R2"
  ip address 50.50.104.2 255.255.255.0
  ipv6 enable
  ipv6 address FE80::2 link-local
  ipv6 address 20FF:CCFF:200D:A1::2/64
  no shutdown
  exit 

 interface Ethernet0/1
  description "to OV-R1"
  ip address 50.50.105.130 255.255.255.128
  ipv6 enable
  ipv6 address FE80::2 link-local
  ipv6 address 20FF:CCFF:200D:A3::2/64
  no shutdown
  exit 

 interface Ethernet0/2
  description "not used"
  no ip address
  shutdown
  exit

 interface Ethernet0/3
  description "not used"
  no ip address
  shutdown
  exit

 key chain EIGRP64501
  key 1
   key-string eigrp64501secretpass
   exit
  exit

 router eigrp EIGRP64501
  address-family ipv4 unicast autonomous-system 1
   eigrp router-id 10.0.3.3
   network 50.50.104.0 0.0.0.255
   network 50.50.105.128 0.0.0.127
   network 10.0.3.3 0.0.0.0
   no shutdown
   af-interface default
    shutdown
    passive-interface
    exit-af-interface
   af-interface Ethernet0/0
    authentication key-chain EIGRP64501
    authentication mode md5
    no shutdown
    no passive-interface
    exit-af-interface
   af-interface Ethernet0/1
    authentication key-chain EIGRP64501
    authentication mode md5
    no shutdown
    no passive-interface
    exit-af-interface 
   af-interface Loopback64501
    authentication key-chain EIGRP64501
    authentication mode md5
    no shutdown
    passive-interface
    exit-af-interface
   topology base
    no auto-summary
    exit-af-topology
   exit-address-family
  address-family ipv6 unicast autonomous-system 1
   eigrp router-id 6.0.3.3
   no shutdown
   af-interface default
    shutdown
    passive-interface
    exit-af-interface
   af-interface Ethernet0/0
    authentication key-chain EIGRP64501
    authentication mode md5
    no shutdown
    no passive-interface
    exit-af-interface
   af-interface Ethernet0/1
    authentication key-chain EIGRP64501
    authentication mode md5
    no shutdown
    no passive-interface
    exit-af-interface  
   af-interface Loopback64501
    authentication key-chain EIGRP64501
    authentication mode md5
    no shutdown
    passive-interface
    exit-af-interface
   topology base
    exit-af-topology
   exit-address-family
  exit
 exit
 